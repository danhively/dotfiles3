# Attempt to get current DIR
DOTFILES_ENTRY="$(dirname "${(%):-%N}")"
#DOTFILES_ENTRY=$HOME/code/dotfiles

# Environment Variables
source $DOTFILES_ENTRY/.env
source $DOTFILES_ENTRY/.env.pub

for dir in $ALIASES $FUNCTIONS $MODULES; do
    for file in "$dir"/.*.sh; do
        [ -f "$file" ] && source "$file"
    done
done
